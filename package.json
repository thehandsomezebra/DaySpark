{
  "name": "dayspark",
  "version": "1.2.2",
  "description": "A digital almanac for daily notes. Injects ICS calendars, sun/moon phases, celestial conjunctions, meteor showers, weather, and historical context.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit && node esbuild.config.mjs production",
    "version": "node scripts/sync-manifest.js && git add manifest.json"
  },
  "keywords": [
    "obsidian",
    "almanac",
    "calendar",
    "ics",
    "astronomy",
    "moon",
    "sun",
    "weather",
    "history",
    "celestial"
  ],
  "author": "Stephanie Frantz",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "esbuild": "0.17.3",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "astronomy-engine": "^2.1.19",
    "ical.js": "^1.5.0"
  }
}
